<?xml version="1.0" encoding="UTF-8"?>
<widget xmlns="http://www.w3.org/ns/widgets"
 xmlns:gap="http://phonegap.com/ns/1.0"
 id="com.tiltandco.phonegap.test01"
 version="1.0.0">
    
    <name>
       Discover New South Wales
    </name>
    
    <description>
        Discover New South Wales
    </description>
    
    <author href="http://www.tiltandco.com/"
     email="shane@titlandco.com">
        Tilt + Co
    </author>
    
    <icon src="resources/icons/icon.png" gap:role="default"/>

    <icon src="resources/icons/icon.png" width="57" height="57" />
    <icon src="resources/icons/icon@2x.png" width="114" height="114" />

    <gap:splash src="resources/loading/Default.png" width="320" height="480" />
    <gap:splash src="resources/loading/Default@2x.png" width="640" height="960" />
    

    <preference name="orientation" value="portrait"/>
    
    <feature name="http://api.phonegap.com/1.0/compass"/>
    <feature name="http://api.phonegap.com/1.0/device"/>
    <feature name="http://api.phonegap.com/1.0/geolocation"/>
    <feature name="http://api.phonegap.com/1.0/media"/>
    <feature name="http://api.phonegap.com/1.0/network"/>
    <feature name="http://api.phonegap.com/1.0/notification"/>
    <feature name="http://api.phonegap.com/1.0/storage"/>

    
    <access origin="*" />
    
</widget>
